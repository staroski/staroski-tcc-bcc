<global-metadata>
	<!-- Specify the bluetooth address of the machine that runs the OBEX TCK Agent -->
	<property name="obex.address" value="127.1.1.1"/>

	<!-- Specify the obex scheme -->
	<property name="obex.scheme" value="tcpobex"/>

    <!-- Specify the port to be used on the machine that runs the OBEX TCK Client -->
    <property name="tcpobex.client.port" value="9500"/>

	<!-- Specify the dynamic dependency files -->
	<dynamicDependancyList>
		<dependancyFile name="com/motorola/tck/tests/api/javax/obex/tcpobex/TestUtilImpl.class"/>
	</dynamicDependancyList>

	<!-- Specify the interval of short pause -->
	<property name="timeout_short" value="2000"/>

	<!-- Specify the interval of medium pause -->
	<property name="timeout_medium" value="5000"/>

	<!-- Specify the interval of long pause -->
	<property name="timeout_long" value="10000"/>

</global-metadata>
